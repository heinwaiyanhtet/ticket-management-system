generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int         @id @default(autoincrement())
  status     user_status
  name       String      @db.VarChar(255)
  department String?     @db.VarChar(255)
  employeeid Int @unique
}

enum user_status {
  admin
  employee
}
