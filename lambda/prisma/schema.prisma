generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int         @id @default(autoincrement())
  status     user_status
  name       String      @db.VarChar(255)
  department String?     @db.VarChar(255)
  employeeid Int
}

enum user_status {
  admin
  employee
}
